created:
    - database/factories/ConferenceFactory.php
    - database/factories/VenueFactory.php
    - database/factories/SpeakerFactory.php
    - database/factories/TalkFactory.php
    - database/migrations/2025_12_02_202309_create_conferences_table.php
    - database/migrations/2025_12_02_202310_create_venues_table.php
    - database/migrations/2025_12_02_202311_create_speakers_table.php
    - database/migrations/2025_12_02_202312_create_talks_table.php
    - database/migrations/2025_12_02_202313_create_conference_speaker_table.php
    - database/migrations/2025_12_02_202314_create_conference_talk_table.php
    - app/Models/Conference.php
    - app/Models/Venue.php
    - app/Models/Speaker.php
    - app/Models/Talk.php
models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable', password: string, remember_token: 'string:100 nullable' }
    Conference: { name: 'string:60', description: text, start_date: datetime, end_date: datetime, status: 'enum:draft,published,archived', region: string, venue_id: 'unsignedInteger nullable', is_published: 'boolean:false', relationships: { belongsTo: Venue, belongsToMany: 'Speaker, Talk' } }
    Venue: { name: string, city: string, country: string, postal_code: string, relationships: { hasMany: Conference } }
    Speaker: { name: string, email: string, bio: text, twitter_handle: string, relationships: { belongsToMany: Conference } }
    Talk: { title: string, abstract: text, relationships: { belongsTo: Speaker, belongsToMany: Conference } }
